@model List<ASPWEBAPP.Models.Domain.EMP>

@{
}
<h1>Employees</h1>

<table  class="table">
   <thead>
        <tr>
            <th>Id</th>
            <th>Name</th>
            <th>Email</th>
            <th>Salary</th>
            <th>DOB</th>
            <th>Department</th>
        </tr>
   </thead>
   <tbody>
       @foreach(var employee in Model)
        {
           <tr>
                <td>@employee.Id</td>
                <td>@employee.Name</td>
                <td>@employee.Email</td>
                <td>@employee.Salary</td>
                <td>@employee.DOB.ToString("dd-mm-yyyy")</td>
                <td>@employee.Department</td>
                <td> <a href="employees/view/@employee.Id"> view</a>  </td>
           </tr> 
        }
    </tbody>

</table>